<% include partials/header %>

<div class="container">
	
	<h1>
		<p> <%= dig.name %> !</p>	
	</h1>
	<h2>
		<%= dig.address %>
	</h2>


	<img src=" <%= dig.image %> " style="height: 70vh; width: 70vw;">

	<p> <%= dig.description %> </p>

<% dig.comments.forEach(function(comment){ %>
	<p>
		<strong> <%= comment.author %> </strong> - <%= comment.text %>
	</p>
<% }) %>

<% include partials/footer %>
</div>
